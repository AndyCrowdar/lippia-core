<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>Sodimac</documentTitle>

    <!-- report name - displayed at top-nav -->
    <reportName>
      <image>
        <![CDATA[
           <img src='http://cdna.cybermonday.com.ar/uploads/marcas/143/sodimac.jpg' />
          		]]>
      </image>
    </reportName>

    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
          $('.logo-container.blue.darken-2').css('display','none');
          $('div.suite-total-steps span.panel-name').html('Gherkin Lines');
        });
        ]]>
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
                .report-name { padding-left: 0px; } .report-name > img { float: left;height: 80%;margin-left: 0px;margin-top: 0px;width: 115px; }
      ]]>
    </styles>
  </configuration>
</extentreports>